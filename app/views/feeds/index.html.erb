<% content_for :embedded_javascript do %>
(function () {
  var
    Feeds = require('shreds/stores/feeds'),
    ShredsApp = require('shreds/stores/shreds_app');
  Feeds.preload(<%= raw @feeds.to_json %>);
  ShredsApp.preload({ toDisplay: 'feeds' });
})();
<% end %>

<% content_for :title do %>
  <%= title("Feeds") %>
<% end %>

