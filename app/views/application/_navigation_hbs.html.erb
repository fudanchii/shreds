<div class="sidebar-nav-fixed widget-transparent">
  <%= render 'subscribe' %>

<ul class="nav nav-list" id="latest_entries">
    <li class="nav-header breadcrumbs" data-template="breadcrumbs">
    </li>
</ul>
<div class="nav-container" data-template="navigation" template="navigation">
</div>
<%= render 'nav_buttons' %>
</div>

<% content_for :embedded_javascript do %>
(function () {
  var
    Navigation = require('shreds/stores/navigation'),
    Assets = require('shreds/stores/assets');
  Navigation.preload(<%= raw render('navigation_data') %>);
  Assets.preload({
    "spinner16x16": "<%= image_path("spinner.16x16.gif") %>"
  });
})();
<% end %>
