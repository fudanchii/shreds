<div class="well well-large widget widget-transparent feed">
  <h3><%= link_to item.title, feed_newsitem_path(item.feed, item), target: "_blank" %></h3>
  <div class="feed-content">
    <%= summary(item).html_safe %>
  </div>
  <div class="small text-right">
    <%= item.published.to_date %>
  </div>
</div>

